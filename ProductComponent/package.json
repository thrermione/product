{
  "name": "product_component",
  "version": "1.0.0",
  "description": "Product component for Lego.com product detail page",
  "author": "Joey Nguyen",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "seed-db": "node ./seeder/seedingScript.js",
    "reset-db": "mysql -u root <schema.sql",
    "start-server": "nodemon ./server/index.js",
    "test": "NODE_ENV=test jest --watchAll",
    "ci-test": "jest ./__tests__ --testPathIgnorePatterns ./server --ci -i --detectOpenHandles --forceExit",
    "start": "webpack --config webpack.prod.js && node ./server/index.js",
    "dev": "concurrently -k \"npm run start-server\" \"npx webpack -w --config webpack.dev.js\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0",
    "supertest": "^4.0.2",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/dom": "^7.5.6",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^25.5.4",
    "jest-environment-enzyme": "^7.1.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-enzyme": "^7.1.2",
    "react-test-renderer": "^16.13.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "babel-plugin-styled-components"
    ]
  }
}
